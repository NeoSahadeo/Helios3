#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/neosahadeo/Documents/Projects/Helios3/app/node_modules/.pnpm/svelte-check@4.0.4_svelte@4.2.19_typescript@5.6.2/node_modules/svelte-check/bin/node_modules:/home/neosahadeo/Documents/Projects/Helios3/app/node_modules/.pnpm/svelte-check@4.0.4_svelte@4.2.19_typescript@5.6.2/node_modules/svelte-check/node_modules:/home/neosahadeo/Documents/Projects/Helios3/app/node_modules/.pnpm/svelte-check@4.0.4_svelte@4.2.19_typescript@5.6.2/node_modules:/home/neosahadeo/Documents/Projects/Helios3/app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/neosahadeo/Documents/Projects/Helios3/app/node_modules/.pnpm/svelte-check@4.0.4_svelte@4.2.19_typescript@5.6.2/node_modules/svelte-check/bin/node_modules:/home/neosahadeo/Documents/Projects/Helios3/app/node_modules/.pnpm/svelte-check@4.0.4_svelte@4.2.19_typescript@5.6.2/node_modules/svelte-check/node_modules:/home/neosahadeo/Documents/Projects/Helios3/app/node_modules/.pnpm/svelte-check@4.0.4_svelte@4.2.19_typescript@5.6.2/node_modules:/home/neosahadeo/Documents/Projects/Helios3/app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/svelte-check" "$@"
else
  exec node  "$basedir/../../bin/svelte-check" "$@"
fi
